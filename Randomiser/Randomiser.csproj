<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <AssemblyName>OriDeRandomiser</AssemblyName>
    <TargetFramework>net35</TargetFramework>
    <AssemblyTitle>Randomiser</AssemblyTitle>
    <Product>Randomiser</Product>
    <Copyright>Copyright ©  2022</Copyright>
    <OutputPath>bin\$(Configuration)\</OutputPath>
  </PropertyGroup>
  <PropertyGroup Condition=" '$(Configuration)|$(Platform)' == 'Debug|AnyCPU' ">
    <DebugType>full</DebugType>
  </PropertyGroup>
  <PropertyGroup Condition=" '$(Configuration)|$(Platform)' == 'Release|AnyCPU' ">
    <DebugType>pdbonly</DebugType>
    <Optimize>false</Optimize>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="Archipelago.MultiClient.Net" Version="4.2.4" />
    <PackageReference Include="Lib.Harmony" Version="2.0.2" />
    <PackageReference Include="OriDeModLoader" Version="0.8.1-alpha" />
  </ItemGroup>
  <ItemGroup>
    <None Include="Assets\LocationData.json" />
  </ItemGroup>
</Project>